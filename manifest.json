{
  "name": "__MSG_extName__",
  "version": "3.2.9",
  "description": "__MSG_desName__",
  "permissions": [
    "storage"
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self'; script-src 'self'; object-src 'none'"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "overlay.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "icons": {
    "16": "ICON 16.png",
    "32": "ICON 32.png",
    "64": "ICON 64.png",
    "128": "ICON 128.png"
  },
  "manifest_version": 3,
  "default_locale": "en",
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "mac": "Alt+Y",
        "linux": "Ctrl+Shift+J",
        "windows": "Ctrl+Shift+Y"
      }
    },
    "left": {
      "suggested_key": {
        "default": "Ctrl+Shift+Left",
        "mac": "Command+Shift+Left"
      },
      "description": "Decrease Speed"
    },
    "right": {
      "suggested_key": {
        "default": "Ctrl+Shift+Right",
        "mac": "Command+Shift+Right"
      },
      "description": "Increase Speed"
    }
  }
}